    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.0",
    "posthtml-include": "^1.7.4",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5"
  },
  "dependencies": {
    "aos": "^2.3.4"
  }
}
